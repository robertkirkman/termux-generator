+         capabilities |= sun_nio_fs_UnixNativeDispatcher_SUPPORTS_FUTIMES;
+     if (my_lutimes_func != NULL)
+         capabilities |= sun_nio_fs_UnixNativeDispatcher_SUPPORTS_LUTIMES;
